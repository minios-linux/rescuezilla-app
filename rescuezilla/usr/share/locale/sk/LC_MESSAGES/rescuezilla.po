msgstr "Odpojenie:{path}"

#, python-brace-format
msgid "Unable to unmount {path}"
msgstr "Nie je možné odpojiť {path}"

#, python-brace-format
msgid "Running: {app}"
msgstr "Spustená: {app}"

#, python-brace-format
msgid "Running {app} on {device}"
msgstr "Spustená {app} na {device}"

